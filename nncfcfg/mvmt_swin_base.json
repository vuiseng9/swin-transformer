{
    "input_info": {
      "sample_size": [1, 3, 224, 224]
    },
    "compression":
    {
      "algorithm": "movement_sparsity",
      "params": {
        "schedule": "threshold_polynomial_decay",
        "power": 3,
        "init_importance_threshold": -20.0,
        "final_importance_threshold": 0.0,
        "warmup_start_epoch": 10,
        "warmup_end_epoch": 15,
        "steps_per_epoch": 2502,
        "importance_regularization_factor": 0.02,
        "update_per_optimizer_step": true,
      },
      "sparse_structure_by_scopes": [
          ["block", [32, 32], "{re}.*WindowAttention[attn].*"],
          ["per_dim", [0], "{re}.*fc1.*"],
          ["per_dim", [1], "{re}.*fc2.*"]
      ],
    },
    "ignored_scopes": ["{re}.*PatchEmbed.*", "{re}.*PatchMerging.*", "{re}.*unbind.*", "{re}.*head.*"],
}