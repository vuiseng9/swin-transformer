{
    "input_info": {
      "sample_size": [1, 3, 224, 224]
    },
    "compression":
    {
      "algorithm": "quantization",
      "overflow_fix": "disable",
      "initializer": {
        "range": {
          "num_init_samples": 32,
          "type": "percentile",
          "params":
            {
              "min_percentile": 0.01,
              "max_percentile": 99.99
            }
        },
        "batchnorm_adaptation": {
          // "num_bn_adaptation_samples": 2
          "num_bn_adaptation_samples": 200
        }
      },
      "activations":
      {
        "mode": "symmetric"
      },
      "weights":
      {
        "mode": "symmetric",
        "signed": true,
        "per_channel": false
      },
      "ignored_scopes": ["{re}.*unbind*"]
    },
}